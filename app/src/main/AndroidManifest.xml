<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.example.safer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TransferenciaActivity" android:screenOrientation="locked"></activity>
        <activity android:name=".AprenderAEmpreenderActivity" android:screenOrientation="locked"/>
        <activity android:name=".MenuDeOpcoesActivity" android:screenOrientation="locked"/>
        <activity android:name=".CadastroClienteActivity" android:screenOrientation="locked"/>
        <activity android:name=".TelaDeLoginActivity" android:screenOrientation="locked"/>
        <activity android:name=".TelaPrincipalActivity" android:screenOrientation="locked">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>